{
  "name": "linux_webgui",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "bin": "dist/index.js",
  "pkg": {
    "assets": [
      "dist/**/*",
      "client/**/*",
      "node_modules/**/*"
    ],
    "targets": [
      "node16-linux",
      "node16-win"
    ],
    "outputPath": "executable"
  },
  "scripts": {
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "build": "pnpm build:server && pnpm build:client",
    "build:pkg": "pnpm build && pnpm prune --prod && pkg . && node scripts/copy_env.js && pnpm i",
    "dev": "nodemon",
    "dev:client": "vite",
    "start": "node ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "socket.io": "^4.7.5",
    "ssh2": "^1.15.0",
    "uuid4": "^2.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/ssh2": "^1.15.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "nodemon": "^3.1.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router-dom": "^6.23.0",
    "socket.io-client": "^4.7.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.10"
  }
}
